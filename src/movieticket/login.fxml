<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="780.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="movieticket.LoginController">
    <stylesheets>
        <URL value="@login.css" />
    </stylesheets>
   <children>
      <ImageView fitHeight="510.0" fitWidth="395.0" layoutX="-1.0">
         <image>
            <Image url="@icons/theter.jpg" />
         </image>
      </ImageView>
      <AnchorPane layoutX="394.0" prefHeight="510.0" prefWidth="389.0">
         <children>
            <TextField fx:id="uname" layoutX="113.0" layoutY="241.0" prefHeight="36.0" prefWidth="215.0" promptText="Username" style="-fx-border-color: #002171; -fx-background-color: transparent; -fx-border-width: 0 0 2 0;" />
            <PasswordField fx:id="pwd" layoutX="113.0" layoutY="310.0" prefHeight="36.0" prefWidth="215.0" promptText="Password" style="-fx-border-color: #002171; -fx-border-width: 0 0 2 0; -fx-background-color: transparent;" />
            <Button fx:id="login" layoutX="159.0" layoutY="379.0" mnemonicParsing="false" onAction="#loginaction" prefHeight="30.0" prefWidth="109.0" style="-fx-background-color: #002171; -fx-background-radius: 18;" stylesheets="@login.css" text="Login" textFill="#fffefe">
               <font>
                  <Font size="17.0" />
               </font>
            </Button>
            <ImageView fitHeight="143.0" fitWidth="200.0" layoutX="114.0" layoutY="16.0">
               <image>
                  <Image url="@icons/popcon-1.jpg" />
               </image>
            </ImageView>
            <MaterialDesignIconView fill="#121b9e" glyphName="ACCOUNT_CIRCLE" layoutX="84.0" layoutY="271.0" size="25" />
            <MaterialDesignIconView fill="#121b9e" glyphName="LOCK_OUTLINE" layoutX="85.0" layoutY="341.0" size="25" />
            <Label layoutX="174.0" layoutY="182.0" text="Sign In" textFill="#e11515">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
